    "    \n",
    "    # Step 1: Decode content (remove `=3D` and similar encodings)\n",
    "    decoded_content = re.sub(r\"=([0-9A-Fa-f]{2})\", lambda x: chr(int(x.group(1), 16)), payload)\n",
    "    decoded_content = decoded_content.replace(\"=\\r\\n\", \"\").replace(\"\\r\\n\", \" \")\n",
    "    \n",
    "    # Step 2: Remove HTML tags using BeautifulSoup\n",
    "    soup = BeautifulSoup(decoded_content, \"html.parser\")\n",
    "    text_content = soup.get_text()\n",
    "    \n",
    "    # Step 3: Remove inline styles and unnecessary noise\n",
    "    cleaned_content = re.sub(r\"(\\\\r|\\\\n|\\\\t)\", \" \", text_content)  # Remove newlines and tabs\n",
    "    cleaned_content = re.sub(r\"\\s+\", \" \", cleaned_content).strip()  # Remove extra whitespace\n",
    "    cleaned_content = re.sub(r\"http\\S+\", \"\", cleaned_content)  # Remove URLs\n",
    "    cleaned_content = re.sub(r\"[^\\w\\s.,!?']\", \"\", cleaned_content)  # Keep alphanumeric and basic punctuation\n",
    "\n",
    "    return cleaned_content\n",
    "\n",
    "# Reads messages on email server\n",
    "def read_message(imap: imaplib):\n",
    "    imap.select(\"Inbox\")\n",
    "    _, msgnums = imap.search(None, 'ALL')\n",
    "\n",
    "    #print(msgnums)\n",
    "    #for msgnum in msgnums[0].split():\n",
    "    _, data = imap.fetch(str(6).encode(), \"(RFC822)\")\n",
    "\n",
    "    message = email.message_from_bytes(data[0][1])\n",
    "    print(\"Message Number: {}\".format(6))\n",
    "    print(\"From: {}\".format(message.get('From')))\n",
    "    print(\"To: {}\".format(message.get('To')))\n",
    "    print(\"BCC: {}\".format(message.get('BCC')))\n",
    "    print(\"Date: {}\".format(message.get('Date')))\n",
    "    print(\"Subject: {}\".format(message.get('Subject')))\n",
    "    print(\"Content:\")\n",
    "    for part in message.walk():\n",
    "        if part.get_content_type() == \"text/plain\":\n",
    "            payload = part.get_payload(decode=1)\n",
    "            if payload:\n",
    "                cleaned_text = clean_gmail_payload(payload)\n",
    "                print(colored(\"Message----\", 'blue'))\n",
    "                print(cleaned_text[:1000])  # Display the first 1000 characters of cleaned text\n",
    "                email_spam_count = cv.transform([cleaned_text[:1000]])\n",
    "                print(colored(naive_model.predict(email_spam_count), 'magenta'))\n",
    "\n",
    "    imap.close()\n",
    "\n",
    "#Script portion that will attempt to connect to IMAP4 server\n",
    "try:\n",
    "    #imaplib.Debug = 4\n",
    "    print(\"Connecting to the IMAP server\")\n",
    "    imap = imaplib.IMAP4_SSL(imap_server, 993)\n",
    "    print(\"Connection to IMAP server successfull\")\n",
    "    \n",
    "    print(\"Logging in as: {}\".format(str(os.getenv('EMAIL_ADDRESS'))))\n",
    "    imap.login(email_address, app_password)\n",
    "    print(\"Login Successful\")\n",
    "\n",
    "    read_message(imap)\n",
    "except imaplib.IMAP4.error as e:\n",
    "    print(\"IMAP error: {}\".format(e))"
